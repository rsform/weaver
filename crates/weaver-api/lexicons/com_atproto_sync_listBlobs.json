{
  "lexicon": 1,
  "id": "com.atproto.sync.listBlobs",
  "defs": {
    "main": {
      "type": "query",
      "description": "List blob CIDs for an account, since some repo revision. Does not require auth; implemented by PDS.",
      "parameters": {
        "type": "params",
        "required": [
          "did"
        ],
        "properties": {
          "cursor": {
            "type": "string"
          },
          "did": {
            "type": "string",
            "description": "The DID of the repo.",
            "format": "did"
          },
          "limit": {
            "type": "integer",
            "default": 500,
            "minimum": 1,
            "maximum": 1000
          },
          "since": {
            "type": "string",
            "description": "Optional revision of the repo to list blobs since.",
            "format": "tid"
          }
        }
      },
      "output": {
        "encoding": "application/json",
        "schema": {
          "type": "object",
          "required": [
            "cids"
          ],
          "properties": {
            "cids": {
              "type": "array",
              "items": {
                "type": "string",
                "format": "cid"
              }
            },
            "cursor": {
              "type": "string"
            }
          }
        }
      },
      "errors": [
        {
          "name": "RepoNotFound"
        },
        {
          "name": "RepoTakendown"
        },
        {
          "name": "RepoSuspended"
        },
        {
          "name": "RepoDeactivated"
        }
      ]
    }
  }
}