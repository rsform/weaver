{
  "lexicon": 1,
  "id": "chat.bsky.convo.sendMessageBatch",
  "defs": {
    "batchItem": {
      "type": "object",
      "required": [
        "convoId",
        "message"
      ],
      "properties": {
        "convoId": {
          "type": "string"
        },
        "message": {
          "type": "ref",
          "ref": "chat.bsky.convo.defs#messageInput"
        }
      }
    },
    "main": {
      "type": "procedure",
      "input": {
        "encoding": "application/json",
        "schema": {
          "type": "object",
          "required": [
            "items"
          ],
          "properties": {
            "items": {
              "type": "array",
              "items": {
                "type": "ref",
                "ref": "#batchItem"
              },
              "maxLength": 100
            }
          }
        }
      },
      "output": {
        "encoding": "application/json",
        "schema": {
          "type": "object",
          "required": [
            "items"
          ],
          "properties": {
            "items": {
              "type": "array",
              "items": {
                "type": "ref",
                "ref": "chat.bsky.convo.defs#messageView"
              }
            }
          }
        }
      }
    }
  }
}