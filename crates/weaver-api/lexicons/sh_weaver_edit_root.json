{
  "lexicon": 1,
  "id": "sh.weaver.edit.root",
  "defs": {
    "main": {
      "type": "record",
      "description": "The starting point for edit history on a notebook.",
      "key": "tid",
      "record": {
        "type": "object",
        "required": [
          "doc",
          "snapshot"
        ],
        "properties": {
          "doc": {
            "type": "ref",
            "ref": "sh.weaver.edit.defs#docRef"
          },
          "snapshot": {
            "type": "blob",
            "accept": [
              "*/*"
            ],
            "maxSize": 30000000
          }
        }
      }
    }
  }
}