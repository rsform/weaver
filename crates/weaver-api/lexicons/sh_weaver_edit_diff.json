{
  "lexicon": 1,
  "id": "sh.weaver.edit.diff",
  "defs": {
    "main": {
      "type": "record",
      "description": "An edit record for a notebook.",
      "key": "tid",
      "record": {
        "type": "object",
        "required": [
          "root",
          "doc"
        ],
        "properties": {
          "doc": {
            "type": "ref",
            "ref": "sh.weaver.edit.defs#docRef"
          },
          "inlineDiff": {
            "type": "bytes",
            "description": "An inline diff for for small edit batches. Either this or snapshot must be present to be valid",
            "maxLength": 8192
          },
          "prev": {
            "type": "ref",
            "ref": "com.atproto.repo.strongRef"
          },
          "root": {
            "type": "ref",
            "ref": "com.atproto.repo.strongRef"
          },
          "snapshot": {
            "type": "blob",
            "description": "Diff from previous diff. Either this or inlineDiff must be present to be valid",
            "accept": [
              "*/*"
            ],
            "maxSize": 3000000
          }
        }
      }
    }
  }
}