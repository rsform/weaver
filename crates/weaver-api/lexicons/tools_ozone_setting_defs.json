{
  "lexicon": 1,
  "id": "tools.ozone.setting.defs",
  "defs": {
    "option": {
      "type": "object",
      "required": [
        "key",
        "value",
        "did",
        "scope",
        "createdBy",
        "lastUpdatedBy"
      ],
      "properties": {
        "createdAt": {
          "type": "string",
          "format": "datetime"
        },
        "createdBy": {
          "type": "string",
          "format": "did"
        },
        "description": {
          "type": "string",
          "maxLength": 10240,
          "maxGraphemes": 1024
        },
        "did": {
          "type": "string",
          "format": "did"
        },
        "key": {
          "type": "string",
          "format": "nsid"
        },
        "lastUpdatedBy": {
          "type": "string",
          "format": "did"
        },
        "managerRole": {
          "type": "string",
          "knownValues": [
            "tools.ozone.team.defs#roleModerator",
            "tools.ozone.team.defs#roleTriage",
            "tools.ozone.team.defs#roleAdmin",
            "tools.ozone.team.defs#roleVerifier"
          ]
        },
        "scope": {
          "type": "string",
          "knownValues": [
            "instance",
            "personal"
          ]
        },
        "updatedAt": {
          "type": "string",
          "format": "datetime"
        },
        "value": {
          "type": "unknown"
        }
      }
    }
  }
}