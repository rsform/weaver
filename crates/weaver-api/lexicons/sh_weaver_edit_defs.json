{
  "lexicon": 1,
  "id": "sh.weaver.edit.defs",
  "defs": {
    "docRef": {
      "type": "object",
      "required": [
        "value"
      ],
      "properties": {
        "value": {
          "type": "union",
          "refs": [
            "#notebookRef",
            "#entryRef",
            "#draftRef"
          ]
        }
      }
    },
    "draftRef": {
      "type": "object",
      "required": [
        "draft_key"
      ],
      "properties": {
        "draft_key": {
          "type": "string",
          "maxLength": 200
        }
      }
    },
    "entryRef": {
      "type": "object",
      "required": [
        "entry"
      ],
      "properties": {
        "entry": {
          "type": "ref",
          "ref": "com.atproto.repo.strongRef"
        }
      }
    },
    "notebookRef": {
      "type": "object",
      "required": [
        "notebook"
      ],
      "properties": {
        "notebook": {
          "type": "ref",
          "ref": "com.atproto.repo.strongRef"
        }
      }
    }
  }
}