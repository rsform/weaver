{
  "lexicon": 1,
  "id": "tools.ozone.team.listMembers",
  "defs": {
    "main": {
      "type": "query",
      "description": "List all members with access to the ozone service.",
      "parameters": {
        "type": "params",
        "properties": {
          "cursor": {
            "type": "string"
          },
          "disabled": {
            "type": "boolean"
          },
          "limit": {
            "type": "integer",
            "default": 50,
            "minimum": 1,
            "maximum": 100
          },
          "q": {
            "type": "string"
          },
          "roles": {
            "type": "array",
            "items": {
              "type": "string"
            }
          }
        }
      },
      "output": {
        "encoding": "application/json",
        "schema": {
          "type": "object",
          "required": [
            "members"
          ],
          "properties": {
            "cursor": {
              "type": "string"
            },
            "members": {
              "type": "array",
              "items": {
                "type": "ref",
                "ref": "tools.ozone.team.defs#member"
              }
            }
          }
        }
      }
    }
  }
}