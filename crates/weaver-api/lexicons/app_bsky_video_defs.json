{
  "lexicon": 1,
  "id": "app.bsky.video.defs",
  "defs": {
    "jobStatus": {
      "type": "object",
      "required": [
        "jobId",
        "did",
        "state"
      ],
      "properties": {
        "blob": {
          "type": "blob"
        },
        "did": {
          "type": "string",
          "format": "did"
        },
        "error": {
          "type": "string"
        },
        "jobId": {
          "type": "string"
        },
        "message": {
          "type": "string"
        },
        "progress": {
          "type": "integer",
          "description": "Progress within the current processing state.",
          "minimum": 0,
          "maximum": 100
        },
        "state": {
          "type": "string",
          "description": "The state of the video processing job. All values not listed as a known value indicate that the job is in process.",
          "knownValues": [
            "JOB_STATE_COMPLETED",
            "JOB_STATE_FAILED"
          ]
        }
      }
    }
  }
}