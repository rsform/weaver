{
  "lexicon": 1,
  "id": "sh.weaver.graph.getPopularTags",
  "defs": {
    "main": {
      "type": "query",
      "description": "Get most-used tags overall.",
      "parameters": {
        "type": "params",
        "properties": {
          "cursor": {
            "type": "string"
          },
          "limit": {
            "type": "integer",
            "default": 50,
            "minimum": 1,
            "maximum": 100
          }
        }
      },
      "output": {
        "encoding": "application/json",
        "schema": {
          "type": "object",
          "required": [
            "tags"
          ],
          "properties": {
            "cursor": {
              "type": "string"
            },
            "tags": {
              "type": "array",
              "items": {
                "type": "ref",
                "ref": "sh.weaver.graph.defs#tagView"
              }
            }
          }
        }
      }
    }
  }
}