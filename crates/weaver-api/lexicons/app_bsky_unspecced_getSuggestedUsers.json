{
  "lexicon": 1,
  "id": "app.bsky.unspecced.getSuggestedUsers",
  "defs": {
    "main": {
      "type": "query",
      "description": "Get a list of suggested users",
      "parameters": {
        "type": "params",
        "properties": {
          "category": {
            "type": "string",
            "description": "Category of users to get suggestions for."
          },
          "limit": {
            "type": "integer",
            "default": 25,
            "minimum": 1,
            "maximum": 50
          }
        }
      },
      "output": {
        "encoding": "application/json",
        "schema": {
          "type": "object",
          "required": [
            "actors"
          ],
          "properties": {
            "actors": {
              "type": "array",
              "items": {
                "type": "ref",
                "ref": "app.bsky.actor.defs#profileView"
              }
            }
          }
        }
      }
    }
  }
}