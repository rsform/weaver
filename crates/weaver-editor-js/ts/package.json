{
  "name": "@weaver.sh/editor-core",
  "version": "0.1.0",
  "description": "Embeddable markdown editor for Weaver.sh",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
